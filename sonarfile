pipeline{
    agent any
    stages{
        stage("sonar scan"){
            agent{
                docker{
                    image 'maven'
                }
            }
            steps{
                script{
                    withSonarQubeEnv(credentialsId: 'sonar') {
                       sh 'printenv'
                       sh 'mvn sonar:sonar'
                     }
                }
            
            }
        }
    }
}

